    "exclude": ["cjs/", "esm/**/*", "node_modules/"]
  },
  "imports": {
    "ts-essentials": "npm:ts-essentials@^10.0.2",
    "yaml": "npm:yaml@^2.7.1"
  },
  "exclude": ["cjs/", "esm/**/*", "node_modules/"],
  "publish": {
    "include": ["src/**/*.ts", "README.md", "LICENSE"]
  }
